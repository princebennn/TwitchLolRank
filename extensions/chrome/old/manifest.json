{
    "manifest_version": 2,

    "name": "TwitchLolRank",
    "description": "This extension shows League of Legends ranks on Twitch chat.",
    "version": "1.0",
    "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
    },
    "content_scripts": [
	{
	  "matches": ["https://*.twitch.tv/*"],
	  "css": ["mystyles.css"],
	  "js": ["myscript.js"]
	}
    ]
}
